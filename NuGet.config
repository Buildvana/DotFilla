<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="Tenacom" value="https://f.feedz.io/tenacom/private/nuget/index.json" />
    <add key="Tenacom preview" value="https://f.feedz.io/tenacom/preview/nuget/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <Tenacom>
      <add key="Username" value="tenacom" />
      <add key="ClearTextPassword" value="%FEEDZ_IO_PRIVATE_TOKEN%" />
    </Tenacom>
  </packageSourceCredentials>
  <packageSourceMapping>
    <packageSource key="Tenacom preview">
      <package pattern="*" />
    </packageSource>
    <packageSource key="Tenacom">
      <package pattern="Tenacom.*" />
    </packageSource>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
